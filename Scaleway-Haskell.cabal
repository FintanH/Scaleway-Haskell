-- description:
homepage:            https://github.com/githubuser/Scaleway-Haskell#readme
license:             BSD3
license-file:        LICENSE
author:              Author name here
maintainer:          example@example.com
copyright:           2017 Author name here
category:            Web
build-type:          Simple
extra-source-files:  README.md
cabal-version:       >=1.10

executable test
  hs-source-dirs:    test, src
  main-is:           Main.hs
  build-depends:     base >= 4.7 && < 5
                  , text==1.2.2.1
                  , aeson==1.0.2.1
                  , time==1.6.0.1
                  , bytestring==0.10.8.1
                  , cases==0.1.3.2
                  , unordered-containers==0.2.8.0
                  , lens==4.15.1
                  , wreq==0.5.0.0
  default-language:  Haskell2010

library
  hs-source-dirs:      src
  exposed-modules:     Scaleway
                     , Scaleway.Types.Types
                     , Scaleway.Network.Servers
  build-depends:       base >= 4.7 && < 5
                     , text==1.2.2.1
                     , aeson==1.0.2.1
                     , time==1.6.0.1
                     , bytestring==0.10.8.1
                     , cases==0.1.3.2
                     , unordered-containers==0.2.8.0
                     , lens==4.15.1
                     , wreq==0.5.0.0
  default-language:    Haskell2010

source-repository head
  type:     git
  location: https://github.com/githubuser/Scaleway-Haskell
